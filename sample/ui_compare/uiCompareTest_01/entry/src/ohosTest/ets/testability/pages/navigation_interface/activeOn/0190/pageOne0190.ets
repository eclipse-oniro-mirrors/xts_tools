@Builder
export function PageOne0190Builder(name: string, param: Object) {
  PageOne0190()
}

@Component
export struct PageOne0190 {
  pathStack: NavPathStack = new NavPathStack();

  build() {
    NavDestination() {
      Column() {
        Text('PageOne')
          .fontSize(24)
          .fontWeight(FontWeight.Bold)
          .margin({ bottom: 20 });

        Button('pushPathByName pageThree animated false')
          .id('pushPathByNamePageThreeAnimatedFalseBtn0190')
          .width(300)
          .height(50)
          .margin({ bottom: 10 })
          .onClick(() => {
            this.pathStack.pushPathByName('PageThree0190', null, false);
          });

        Button('pushDestinationByName pageTwo animated false')
          .id('pushDestinationByNamePageTwoAnimatedFalseBtn0190')
          .width(300)
          .height(50)
          .margin({ bottom: 10 })
          .onClick(() => {
            this.pathStack.pushDestinationByName('PageTwo0190', null, false);
          });

        Button('pop animated false')
          .id('popAnimatedFalseBtn0190')
          .width(300)
          .height(50)
          .margin({ bottom: 10 })
          .onClick(() => {
            this.pathStack.pop(false);
          });

        Button('moveIndexToTop 0 animated false')
          .id('moveIndexToTop0AnimatedFalseBtn0190')
          .width(300)
          .height(50)
          .margin({ bottom: 10 })
          .onClick(() => {
            this.pathStack.moveIndexToTop(0, false);
          });
      }
      .width('100%')
      .height('100%')
      .padding(20)
      .justifyContent(FlexAlign.Center)
      .backgroundColor('#F5F5F5')
    }
    .title('PageOne')
    .mode(NavDestinationMode.STANDARD)
    .onActive((reason: number) => {
      console.log(`testTag PageOne0190 onActive reason: ${reason}`);
    })
    .onInactive((reason: number) => {
      console.log(`testTag PageOne0190 onInActive reason: ${reason}`);
    })
    .onReady((context: NavDestinationContext) => {
      this.pathStack = context.pathStack;
    })
  }
}

